<!DOCTYPE html>
<html>
  <head>
    <title>ChoroStore</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag "style" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
  <div class="container-fluid no-padding">
    <nav>
      <div class="col-md-12 no-padding">
        <div class="col-md-2 col-sm-10 no-padding">
          <a href="#"><img class="img-responsive"src="/images/logo2.png" alt="logo" class="logo"></a>
          <!--<a href="#"><img src="https://scontent.fscl3-1.fna.fbcdn.net/v/t1.0-1/p200x200/16640673_985017418298768_6735879098055262724_n.png?oh=5692627c3295fe21977c787cfff158a0&oe=59251F21" alt="logo" class="logo"></a>-->
        </div>
        <div class="col-md-10 col-sm-2 no-padding navmenu">
          <ul class="col-md-12 no-padding row">
            <li class="btn col-md-3 col-md-offset-1"><%= link_to 'Quienes Somos', root_path %></li>
            <li class="btn col-md-3"><%= link_to 'Catalogo', root_path %></li>
            <li class="btn col-md-3"><%= link_to 'Contacto', root_path %></li>
            <li class="btn col-md-1 "><%= link_to 'Login', root_path %></li>
            <li class="btn col-md-1"><%= link_to 'New User', root_path %></li>
          </ul>
        </div>
      </div>
    </nav>
    <aside class="col-md-2">
      <div class="row">
        <ul>
          <li class="btn"><%= link_to 'Aniversarios', root_path %></li>
          <li class="btn"><%= link_to 'Nacimientos', root_path %></li>
          <li class="btn"><%= link_to 'Cumpleaños', root_path %></li>
          <li class="btn"><%= link_to 'Bodas/Novios', root_path %></li>
          <li class="btn"><%= link_to 'Condolencias', root_path %></li>
        </ul>
      </div>
    </aside>
    <div class="col-md-10">
        <p class="notice"><%= notice %></p>
       <p class="alert"><%= alert %></p>
      <%= yield %>
    </div>
    <footer class="col-lg-12 center text-center">
      <p> Keilabs ®2017</p>
    </footer>
  </div>
  </body>

</html>
